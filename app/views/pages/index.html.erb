<h1 class="text-center my-5">Twitter Index</h1>

<div class="card">
       <div class="card-body">
              <%= form_with(model: @tweet, local: true) do |f| %>
                     <%= f.label "Twitea algo!" %><br>
                     <%= f.text_field :content %><br>
                     <%= f.submit "Subir" %>
              <%end%>
       </div>
</div>