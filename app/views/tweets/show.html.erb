<% pre_like = @tweet.likes.find {|like| like.user_id == current_user.id} %>
       <% if pre_like %>
              <%= button_to "Unlike", tweet_like_path(@tweet, pre_like), method: :delete %>
       <%else%>
              <%= button_to 'Like', tweet_likes_path(@tweet), method: :tweet %>
       <%end%>

<%= link_to "Atras", root_path %>